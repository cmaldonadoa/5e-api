{
  "type": "module",
  "prettier": {
    "trailingComma": "es5"
  },
  "scripts": {
    "compile": "npm run generate-structure && npm run generate-types && tsc",
    "start": "npm run compile && node --experimental-specifier-resolution=node ./dist/index.js",
    "generate-structure": "tsx ./scripts/generate-structure.ts",
    "generate-schema": "tsx ./scripts/json2graphql.ts",
    "generate-schemas": "tsx ./scripts/generate-schemas.ts",
    "generate-data": "tsx ./scripts/generate-data.ts",
    "generate-types": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@graphql-tools/load-files": "^7.0.0",
    "app-root-dir": "^1.0.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-resolvers": "4.0.6",
    "@types/app-root-dir": "^0.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.11.19",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
